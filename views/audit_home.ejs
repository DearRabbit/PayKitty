<!DOCTYPE html>
<html lang="zh-CN">
  <head>
    <% include audit_header.ejs %>
  </head>
  <body class="nav-md">
    <div class="container body">
      <div class="main_container">
        <% include audit_common.ejs%>

        <div class="right_col" role="main">
          <div class="">

            <!-- title -->
            <div class="page-title">
              <div class="title_left">
                <h3>
                  审核账单
                </h3>
              </div>
              <div class="title_right">
                <h1>&nbsp;</h1>
              </div>
            </div>
            <!-- title -->

            <div class="row">
              <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>2016-05-13</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                      </li>
                      <li><a class="close-link"><i class="fa fa-close"></i></a>
                      </li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <table id="datatable" class="table table-striped table-bordered">
                      <thead>
                      <tr>
                        <th>订单号</th>
                        <th>买家ID</th>
                        <th>卖家ID</th>
                        <th>订单状态</th>
                        <th>交易金额</th>
                        <th>审核状态</th>
                      </tr>
                      </thead>

                      <tbody>
                      <% for (var i in data) { %>
                      <tr>
                        <td><%= data[i].id %></td>
                        <td><%= data[i].buyer %></td>
                        <td><%= data[i].seller %></td>
                        <td><%= data[i].status %></td>
                        <td><%= data[i].money %></td>
                        <td><%= data[i].audit %></td>
                      </tr>
                      <% } %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>

          </div>
        </div>

        <% include audit_footer.ejs%>
      </div>
    </div>
  <% include audit_footjs.ejs %>
    <!-- Datatables -->
    <script>
      $('#datatable').dataTable({
        language: {
          url: '//cdn.datatables.net/plug-ins/1.10.11/i18n/Chinese.json'
        }
      });
    </script>
    <!-- /Datatables -->
  </body>
</html>